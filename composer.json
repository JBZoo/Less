{
    "name"              : "jbzoo/less",
    "type"              : "library",
    "description"       : "PHP wrapper for any less-compilers",
    "license"           : "MIT",
    "keywords"          : [
        "less", "css", "less.js", "stylesheet",
        "jbzoo", "packagist", "lessc", "gpeasy", "leafo", "lesscss", "compiler"
    ],
    "minimum-stability" : "stable",
    "authors"           : [
        {
            "name"  : "SmetDenis",
            "email" : "denis@jbzoo.com",
            "role"  : "lead"
        }
    ],
    "require"           : {
        "php"         : ">=5.4",
        "jbzoo/data"  : "^1.3.4|^2.0",
        "jbzoo/utils" : "^1.4.5|^2.0"
    },
    "require-dev"       : {
        "jbzoo/phpunit"     : "^1.0",
        "oyejorge/less.php" : "^1.7"
    },
    "suggest"           : {
        "oyejorge/less.php" : "^1.7",
        "mishal/iless"      : "^2.1.0",
        "leafo/lessphp"     : "^0.5"
    },
    "autoload"          : {
        "psr-4" : {
            "JBZoo\\Less\\" : "src"
        }
    },
    "extra"             : {
        "branch-alias" : {
            "dev-master" : "1.x-dev"
        }
    },
    "config"            : {
        "optimize-autoloader" : true
    },
    "support"           : {
        "issues" : "https://github.com/JBZoo/Less/issues"
    }
}
